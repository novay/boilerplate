{
    "name": "novay/boilerplate",
    "description": "My personal Laravel boilerplate.",
    "keywords": ["laravel", "splade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media"
        },
        {
            "name": "Novianto Rahmadi",
            "email": "novay@btekno.id"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.37|^10.0",
        "illuminate/filesystem": "^9.37|^10.0",
        "illuminate/support": "^9.37|^10.0",
        "illuminate/validation": "^9.37|^10.0",
        "protonemedia/laravel-splade": "^1.3.0"
    },
    "conflict": {
        "laravel/framework": "<9.37.0"
    },
    "autoload": {
        "psr-4": {
            "Novay\\Boilerplate\\": "src/"
        }, 
        "files": [
            "src/Helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Novay\\Boilerplate\\BoilerplateServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.1"
    }
}